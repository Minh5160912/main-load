-- üåê Universal Auto Script Loader (fixed)
loadstring(game:HttpGet("https://raw.githubusercontent.com/Minh5160912/main-load/refs/heads/main/intro%20load",true))()

wait(1.0)
local StarterGui = game:GetService("StarterGui")
local GameId = game.PlaceId

-- Danh s√°ch c√°c game & script
local Scripts = {
    [2753915549] = "https://raw.githubusercontent.com/Minh5160912/minh-beo/refs/heads/main/bf", -- Blox Fruit Sea 1
    [4442272183] = "https://raw.githubusercontent.com/Minh5160912/minh-beo/refs/heads/main/bf", -- Blox Fruit Sea 2
    [7449423635] = "https://raw.githubusercontent.com/Minh5160912/minh-beo/refs/heads/main/bf", -- Blox Fruit Sea 3
    [3623096087] = "https://raw.githubusercontent.com/AhmadV99/Script-Games/main/Muscle%20Legends.lua", -- Muscle Legends
    [10449761463] = "https://raw.githubusercontent.com/AhmadV99/Script-Games/main/The%20Strongest%20Battleground.lua", -- The Strongest Battlegrounds
    [18901165922] = "https://raw.githubusercontent.com/AhmadV99/Script-Games/main/Pet%20GO.lua", -- Pet GO
