}

print("Detected PlaceId:", GameId)

-- H√†m hi·ªÉn th·ªã th√¥ng b√°o an to√†n
local function Notify(title, text, duration)
    pcall(function()
        StarterGui:SetCore("SendNotification", {
            Title = title;
            Text = text;
            Duration = duration or 6;
        })
    end)
end

-- Ki·ªÉm tra v√† load script
if Scripts[GameId] then
    Notify(" Loading Script...", "GameID: "..GameId, 4)
    local success, result = pcall(function()
        loadstring(game:HttpGet(Scripts[GameId]))()
    end)
    if success then
        Notify(" Script Loaded!", "ƒê√£ load script th√†nh c√¥ng!", 5)
    else
        Notify(" L·ªói khi ch·∫°y script!", tostring(result), 8)
        warn(result)
    end
else
    Notify(" Game ch∆∞a ƒë∆∞·ª£c h·ªó tr·ª£!", "ID: "..tostring(GameId), 8)
    warn("‚ö†Ô∏è Game ch∆∞a c√≥ script h·ªó tr·ª£!")
    warn("üëâ B·∫°n c√≥ th·ªÉ th√™m d√≤ng n√†y v√†o b·∫£ng Scripts ƒë·ªÉ h·ªó tr·ª£ game n√†y:")
    warn(string.format("[%s] = \"https://pastebin.com/raw/YOUR_SCRIPT_LINK\", -- %s", tostring(GameId), game:GetService("MarketplaceService"):GetProductInfo(GameId).Name or "Unknown Game"))
end
print("All supported PlaceIds:")
for id, _ in pairs(Scripts) do
    print(" -", id)
end
print("Your current PlaceId:", GameId)

